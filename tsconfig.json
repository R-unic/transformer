{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"outDir": "out",
		"rootDir": "src",
		"strict": true,
		"lib": ["ES2015", "DOM"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"downlevelIteration": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true
	}
}