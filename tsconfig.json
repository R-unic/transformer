{
	"compilerOptions": {
		"target": "ES2015",
		"module": "commonjs",
		"outDir": "out",
		"rootDir": "src",
		"strict": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"downlevelIteration": true,
		"lib": [
			"ES2015",
			"DOM"
		],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
